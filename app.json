{
  "expo": {
    "name": "MughalPVC",
    "slug": "MughalPVC",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/adaptive-icon4.png",
    "scheme": "mughalpvc",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
"android": {
  "adaptiveIcon": {
    "foregroundImage": "./assets/images/adaptive-icon4.png",
    "backgroundColor": "#ffffff"
  },
  "jsEngine": "hermes",
  "edgeToEdgeEnabled": true,
  "package": "com.faiziop05.MughalPVC"
},
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "80b34088-8a94-4a76-a0bd-f8443a761b92"
      }
    }
  }
}
